on","references":[]},{"id":"politics/conservatives","webTitle":"Conservatives","type":"keyword","sectionId":"politics","sectionName":"Politics","webUrl":"http://www.theguardian.com/politics/conservatives","apiUrl":"http://beta.content.guardianapis.com/politics/conservatives","references":[]},{"id":"politics/liberaldemocrats","webTitle":"Liberal Democrats","type":"keyword","sectionId":"politics","sectionName":"Politics","webUrl":"http://www.theguardian.com/politics/liberaldemocrats","apiUrl":"http://beta.content.guardianapis.com/politics/liberaldemocrats","references":[]},{"id":"politics/labour","webTitle":"Labour","type":"keyword","sectionId":"politics","sectionName":"Politics","webUrl":"http://www.theguardian.com/politics/labour","apiUrl":"http://beta.content.guardianapis.com/politics/labour","references":[]},{"id":"uk/uk","webTitle":"UK news","type":"keyword","sectionId":"uk-news","sectionName":"UK news","webUrl":"http://www.theguardian.com/uk/uk","apiUrl":"http://beta.content.guardianapis.com/uk/uk","references":[]},{"id":"society/society","webTitle":"Society","type":"keyword","sectionId":"society","sectionName":"Society","webUrl":"http://www.theguardian.com/society/society","apiUrl":"http://beta.content.guardianapis.com/society/society","references":[]},{"id":"society/localgovernment","webTitle":"Local government","type":"keyword","sectionId":"society","sectionName":"Society","webUrl":"http://www.theguardian.com/society/localgovernment","apiUrl":"http://beta.content.guardianapis.com/society/localgovernment","references":[]},{"id":"politics/localgovernment","webTitle":"Local politics","type":"keyword","sectionId":"politics","sectionName":"Politics","webUrl":"http://www.theguardian.com/politics/localgovernment","apiUrl":"http://beta.content.guardianapis.com/politics/localgovernment","references":[]},{"id":"type/article","webTitle":"Article","type":"type","webUrl":"http://www.theguardian.com/articles","apiUrl":"http://beta.content.guardianapis.com/type/article","re