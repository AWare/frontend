+            params : {
+                accountId : 14062,
+                siteId : 70608,
+                zoneId : 335918,
+                // Optional param - for initial tests
+                sizes: [57] // 970x250
+            }
+        }];
+    }
+
+    function setDFPTargeting() {
+        pbjs.bidderSettings = {
+            standard : {
+                alwaysUseBid : false,
+                adserverTargeting : [{
+                    key : 'hb_pb',
+                    val : cpmToPriceBucket
+                }, {
+                    key : 'hb_adid',
+                    val : function getBidAdId(bidResponse) {
+                        return bidResponse.adId;
+                    }
+                }]
+            }
+        };
+    }
+
+    function cpmToPriceBucket(bidResponse) {
+        var cpm = parseFloat(bidResponse.pbHg);
+
+        if (cpm > 20.00) {
+            return '20.00';
+        } else if (cpm > 5.00) {
+            return bidResponse.pbLg; // .50 increments
+        } else if (cpm > 1.00) {
+            return bidResponse.pbMg; // .10 increments
+        } else {
+            return toFivePenceBucket(cpm).toString();
+        }
+
+        function toFivePenceBucket(pounds) {
+            // Use pence to avoid float manipulation
+            var pence = pounds * 100;
+            var fivePences = Math.floor(pence / 5);
+            return (fivePences * 5) / 100;
+        }
+    }
+
+    function logError(e) {
+        robust.log('cm-prebid', e);
+    }
+
+});
