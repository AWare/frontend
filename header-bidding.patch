+        window.pbjs.que.push(setDFPTargeting);
+
+        // We only run one auction at a time to prevent network congestion.
+        // Each bid can make multiple concurrent HTTP requests, and there are
+        // surprisingly low ceilings on how many some browsers support.
+        var runningAuction = new Promise.resolve();
+
+        this.loadAdvert = function loadAdvert(advert) {
+            return runningAuction = runningAuction.then(nextAuction).catch(logError);
+
+            function nextAuction() {
+                console.log('running', advert.adSlotId);
+                return auction(advert);
+            }
+        };
+
+        this.refreshAdvert = function refreshAdvert(advert) {
+            // Only used in non-dynamic live blog ads
+            return runningAuction = runningAuction.then(nextAuction).catch(logError);
+
+            function nextAuction() {
+                return reAuction(advert);
+            }
+        };
+    };
+
+    function auction(advert) {
+        return new Promise(function (resolve, reject) {
+            pbjs.que.push(function () {
+                pbjs.addAdUnits([
+                    new PrebidAdUnit(advert)
+                ]);
+                pbjs.requestBids({
+                    timeout : PREBID_TIMEOUT,
+                    bidsBackHandler : function () {
+                        try {
+                            pbjs.setTargetingForGPTAsync([advert.adSlotId]);
+                            window.googletag.display(advert.adSlotId);
+                            resolve();
+                        } catch (e) {
+                            reject(e);
+                        }
+                    }
+                });
+            });
+        });
+    }
+
+    function reAuction(advert) {
+        return new Promise(function (resolve, reject) {
+            pbjs.que.push(function () {
+                pbjs.requestBids({
+                    timeout : PREBID_TIMEOUT,
+                    adUnitCodes : [advert.adSlotId],
+                    bidsBackHandler : function () {
+                        try {
+                            pbjs.setTargetingForGPTAsync([advert.adSlotId]);
+                            window.googletag.pubads().refresh([advert.slot]);
+                            resolve();
+                        } catch (e) {
+                            reject(e);
+                        }
+                    }
+                });
+            });
+        });
+    }
+
+    function PrebidAdUnit(advert) {
+        this.code = advert.adSlotId;
